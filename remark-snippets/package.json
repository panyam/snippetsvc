{
  "name": "remark-snippets",
  "version": "0.0.5",
  "author": "Sriram Panyam",
  "description": "A snippet plugin for MDX where snippets are executed and their outputs caputred allowing for upto date documentation pages.",
  "license": "ISC",
  "main": "index.js",
  "exports": [
    "./index.js"
  ],
  "type": "module",
  "engines": {
    "node": ">=14"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "clean": "rm -Rf lib && jest --clearCache && rm -rf static/lib",
    "prepack": "tsc --noEmit false",
    "build": "npm run prepack",
    "test": "jest --coverage --runInBand",
    "lint": "eslint './' --quiet --ext .ts --resolve-plugins-relative-to .",
    "lintfix": "eslint '*.ts' --quiet --fix --resolve-plugins-relative-to .",
    "format": "prettier --write \"*.ts\" \"*.js\"",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "prepare": "npm run build",
    "prepublishOnly": "echo prepublishOnly...",
    "preversion": "npm run build && npm run lintfix",
    "version": "git add -A .",
    "postversion": "git commit -anm 'Updated patch version' && git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/panyam/snippetsvc/tree/master/plugins/remark-snippets"
  },
  "dependencies": {
    "@types/mdast": "^3.0.10",
    "acorn-jsx": "^5.3.2",
    "snippetsvc": "file:../tssnippets",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.1"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^2.2.1",
    "@types/estree": "^1.0.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.18",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "babel-jest": "^29.3.1",
    "esbuild": "^0.16.12",
    "eslint": "^8.30.0",
    "eslint-plugin-react": "^7.31.11",
    "jest": "^29.3.1",
    "jest-each": "^29.3.1",
    "jest-mock": "^29.3.1",
    "mdast-util-mdx": "^2.0.0",
    "mdx-bundler": "^9.2.1",
    "prettier": "^2.8.1",
    "remark": "^14.0.2",
    "to-vfile": "^7.2.3",
    "ts-jest": "^29.0.3",
    "tsx": "^3.12.1",
    "typescript": "^4.9.4",
    "xdm": "^3.4.0"
  },
  "types": "./index.d.ts",
  "directories": {
    "doc": "docs"
  },
  "keywords": [
    "remark",
    "snippets",
    "documentation"
  ]
}
